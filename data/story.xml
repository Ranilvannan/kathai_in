<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="story_book_sequence" model="ir.sequence">
            <field name="name">Story Book</field>
            <field name="code">story.book</field>
            <field name="prefix">STORY/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="story_language_hindi" model="story.language">
            <field name="name">Hindi</field>
            <field name="code">HINDI</field>
        </record>

    </data>
</odoo>